    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.0.1",
    "lucide-react": "^0.364.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-lazy-load-image-component": "^1.6.0",
